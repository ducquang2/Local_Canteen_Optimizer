<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    >

    <!--Background Colors-->
    <Color x:Key="primaryBackColor">#F5F5F5</Color>
    <Color x:Key="secondaryBackColor">#FFF2E8</Color>
    <Color x:Key="secondaryBackColorGray">#9F9F9E</Color>
    
    <!--Border Colors-->
    <Color x:Key="borderColor">#FC8019</Color>

    <!--Button Colors-->
    <Color x:Key="buttonColorOrange">#FC8019</Color>
    <Color x:Key="buttonColorGreen">#09AA29</Color>
    
    <!--TextBox Colors-->
    <Color x:Key="textColor">#171826</Color>

    <!--Etc-->

</ResourceDictionary>
